<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Vue Hands-on</title>
    <script src="https://cdn.jsdelivr.net/npm/vue@2.5.16/dist/vue.js">
    </script>
  </head>
  <body>
    <div id="hello">
        {{message}}
    </div>
  </body>
</html>

<script>
    const hello = new Vue({
      el:'#hello',
      data: {
        message: 'Hello world!!'        
      }
    })
</script>
